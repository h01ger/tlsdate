#! /bin/sh

set -e

case "$1" in
	abort-install|abort-upgrade)
		dpkg-maintscript-helper mv_conffile \
			/etc/init.d/tlsdate /etc/init.d/tlsdated 0.0.7-1.2 -- "$@"
		dpkg-maintscript-helper mv_conffile \
			/etc/default/tlsdate /etc/default/tlsdated 0.0.7-1.2 -- "$@"
		;;
	*)
		;;
esac

#DEBHELPER#
